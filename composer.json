{
  "name": "naimhowlader/livewire-multiselect",
  "description": "A simple Livewire-based multi-select component for Laravel (no JS required).",
  "keywords": [
    "laravel",
    "livewire",
    "multiselect",
    "component"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Naim Howlader",
      "email": "you@example.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "NaimHowlader\\LivewireMultiselect\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "livewire/livewire": "^2.0 || ^3.0",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "NaimHowlader\\LivewireMultiselect\\LivewireMultiselectServiceProvider"
      ]
    }
  }
}
